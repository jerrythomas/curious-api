{
  "name": "curious-api",
  "version": "0.0.1",
  "description": "Rest api to access multiple databases.",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "test": "tape ./spec/*.spec.js",
    "auto-start": "nodemon server/index.js",
    "cover": "istanbul cover tape ./spec/*.spec.js",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "codecov": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info && codecov",
    "lint": "eslint --fix server/*.js "
  },
  "keywords": [
    "query",
    "database",
    "rest"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jerrythomas/curious-api.git"
  },
  "author": "Jerry Thomas",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "env2": "^2.2.0",
    "hapi": "^16.5.2",
    "hapi-plugin-oracledb": "^3.0.3",
    "hapi-postgres-connection": "^6.4.0",
    "istanbul": "^0.4.5",
    "joi": "^10.6.0",
    "oracledb": "^1.13.1",
    "pg": "^7.3.0",
    "tape": "^4.8.0"
  },
  "devDependencies": {
    "codecov": "^2.3.0",
    "coveralls": "^2.13.1",
    "eslint": "^4.6.1",
    "eslint-config-hapi": "^10.1.0",
    "eslint-fix": "^1.0.3",
    "eslint-plugin-hapi": "^4.0.0",
    "eslint-plugin-import": "^2.7.0",
    "js-beautify": "^1.6.14",
    "nodemon": "^1.12.0",
    "sonarqube-scanner": "^2.0.1"
  }
}
